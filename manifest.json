{
    "manifest_version": 3,
    "name": "Cloockler: no more distractions",
    "version": "0.1.0",
    "description": "Chrome extension that can help you to save time",
    "icons": {
      "128": "/images/icon-128.png"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "css": [
          "scripts/content.css"
        ],
        "js": [
          "scripts/content.js"
        ]
      }
    ],
    "background": {
        "service_worker": "scripts/background.js"
    },
    "permissions": [
      "tabs",
      "storage",
      "nativeMessaging"
    ]
  }