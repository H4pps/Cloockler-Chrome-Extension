{
    "manifest_version": 3,
    "name": "Cloockler: think twice",
    "version": "0.1.0",
    "description": "Blocks unnecessary distractions",
    "icons": {
      "128": "/images/icon-128.png"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "css": [
          "scripts/content.css"
        ],
        "js": [
          "scripts/content.js"
        ]
      }
    ],
    "background": {
        "service_worker": "scripts/background.js"
    },
    "permissions": [
      "tabs",
      "storage"
    ]
  }