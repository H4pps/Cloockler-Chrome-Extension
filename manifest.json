{
    "manifest_version": 3,
    "name": "Cloockler: think twice",
    "version": "1.0.0",
    "description": "Blocks unnecessary distractions",
    "icons": {
      "128": "/images/icon-128.png"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "background": {
        "service_worker": "scripts/background.js"
    },
    "permissions": [
      "tabs",
      "storage"
    ]
  }